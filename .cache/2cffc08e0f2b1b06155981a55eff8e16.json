MIT:\n      return {\n        notes: '',\n        tag: '',\n        type: '',\n        amount: 0,\n        location: '',\n      }\n    default:\n      return state\n  }\n}\n"},"lineCount":null}},"hash":"e5f0455dfdf8deb97949626ec47dfb9a","cacheData":{"env":{}}}